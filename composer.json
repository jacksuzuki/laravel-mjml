{
    "name": "jacksuzuki/laravel-mjml",
    "description": "A package that enables using MJML with Laravel Mailables.",
    "version": "0.3.2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ajitem Sahasrabuddhe",
            "email": "ajitem.s@outlook.com"
        },
        {
            "name": "Shuichi Suzuki",
            "email": "syuszk@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Asahasrabuddhe\\LaravelMJML\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asahasrabuddhe\\LaravelMJML\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asahasrabuddhe\\LaravelMJML\\Providers\\MJMLServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.6",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "require": {
        "symfony/process": "^4.0",
        "soundasleep/html2text": "~0.5"
    }
}
